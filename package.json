{
	"name": "backend",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"format": "prettier --write \"{src,__}/**/*.ts\" --find-config-path",
		"build": "tsc",
		"start": "node dist/index.js",
		"dev": "nodemon",
		"deploy": "yarn build && gcloud app deploy -q",
		"server": "ts-node src/index.ts",
		"test": "NODE_ENV=test jest -i --detectOpenHandles",
		"test:coverage": "NODE_ENV=test jest -i --coverage --detectOpenHandles",
		"postinstall": "yarn build",
		"watch": "nodemon -e ts -w ./src -x npm run watch:serve",
		"watch:serve": "ts-node src/index.ts"
	},
	"dependencies": {
		"@google-cloud/storage": "^1.7.0",
		"@sendgrid/mail": "^6.3.1",
		"axios": "^0.18.0",
		"bcrypt": "^2.0.1",
		"cookie-parser": "~1.4.3",
		"cors": "^2.8.4",
		"crypto-js": "^3.1.9-1",
		"debug": "~2.6.9",
		"dotenv": "^5.0.1",
		"express": "~4.16.0",
		"express-async-errors": "^3.0.0",
		"googleapis": "^27.0.0",
		"handlebars": "^4.0.11",
		"helmet": "^3.13.0",
		"jsonwebtoken": "^8.2.1",
		"mongodb": "^3.1.3",
		"mongoose": "^5.2.8",
		"morgan": "~1.9.0",
		"multer": "^1.3.1",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"reflect-metadata": "^0.1.12",
		"routing-controllers": "^0.7.7",
		"validator": "^10.2.0",
		"yes-https": "^0.0.4"
	},
	"devDependencies": {
		"@types/bcrypt": "^2.0.0",
		"@types/body-parser": "^1.17.0",
		"@types/cookie-parser": "^1.4.1",
		"@types/cors": "^2.8.4",
		"@types/crypto-js": "^3.1.39",
		"@types/express": "^4.11.1",
		"@types/express-session": "^1.15.8",
		"@types/faker": "^4.1.4",
		"@types/google-cloud__storage": "^1.7.0",
		"@types/handlebars": "^4.0.39",
		"@types/helmet": "^0.0.39",
		"@types/http-errors": "^1.6.1",
		"@types/jest": "^23.3.5",
		"@types/jsonwebtoken": "^7.2.7",
		"@types/mailgun-js": "^0.16.0",
		"@types/mongodb": "^3.1.4",
		"@types/mongoose": "^5.2.5",
		"@types/morgan": "^1.7.35",
		"@types/multer": "^1.3.7",
		"@types/node": "^10.12.15",
		"@types/passport": "^0.4.6",
		"@types/passport-jwt": "^3.0.1",
		"@types/supertest": "^2.0.6",
		"@types/validator": "^9.4.1",
		"faker": "^4.1.0",
		"jest": "^23.6.0",
